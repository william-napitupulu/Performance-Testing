APP_NAME="Performance Testing"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# -------------------------------------------------
# DATABASE CONNECTIONS
# -------------------------------------------------

# This is the application's default database connection.
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=pt
DB_USERNAME=root
DB_PASSWORD=

# This is the secondary connection for the 'soket' database.
DB_SOKET_HOST=127.0.0.1
DB_SOKET_PORT=3306
DB_SOKET_DATABASE=db_soket
DB_SOKET_USERNAME=root
DB_SOKET_PASSWORD=

# This is the secondary connection for the 'pacitan' database.
DB_PACITAN_HOST=127.0.0.1
DB_PACITAN_PORT=3306
DB_PACITAN_DATABASE=db_pacitan_1
DB_PACITAN_USERNAME=root
DB_PACITAN_PASSWORD=

# -------------------------------------------------
# FRAMEWORK DEFAULTS
# -------------------------------------------------

BROADCAST_CONNECTION=log
CACHE_STORE=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=redis
SESSION_DRIVER=file
SESSION_LIFETIME=120

# -------------------------------------------------
# MAILERS / REDIS / AWS
# -------------------------------------------------

REDIS_CLIENT=predis
REDIS_HOST=redis
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# -------------------------------------------------
# CUSTOM SERVICES
# -------------------------------------------------

# The IP address of the Windows server that runs the Excel analysis.
# Example: http://192.168.1.50
ANALYSIS_SERVER_IP=
ANALYSIS_SERVER_TOKEN=

VITE_APP_NAME="${APP_NAME}"
VITE_APP_URL="${APP_URL}"